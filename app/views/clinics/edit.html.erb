<% @page_title = "クリニック情報の編集" %>

<h1><%= @page_title %></h1>

<div class="row">

<p><%= link_to "クリニックの詳細に戻る", user_clinic_url(id: @clinic.user_id) %></p>

<%= form_for([@user, @clinic]) do |form| %>
<%= render "form", form: form %>
<p><%= button_tag "更新", class: "btn waves-effect waves-light btn", type: "button", onclick: "submit();" %></p>
<% end %>

</div>
