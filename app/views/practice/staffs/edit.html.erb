<% @page_title = "スタッフの編集" %>

<h1><%= @page_title %></h1>

<div class="row">

<p><%= link_to "スタッフ一覧に戻る", user_staffs_url(@staff) %></p>

<%= form_for [@user, @staff] do |form| %>
<%= render "form", form: form %>
<p><%= button_tag "更新", class: "btn waves-effect waves-light", type: "button", onclick: "submit();" %></p>
<% end %>

</div>
