<% @page_title = "クリニックの新規登録" %>

<h1><%= @page_title %></h1>

<p><%= link_to "アカウント詳細に戻る", practice_user_path(@user, @clinic) %></li></p>

<div class="row">

  <%= form_for [:practice, @user, @clinic], url: practice_user_clinic_path do |form| %>

  <%= render "form", form: form %>

  <p><%= button_tag "登録", class: "btn waves-effect waves-light btn", type: "button", onclick: "submit();" %></p>
  <% end %>

</div>
